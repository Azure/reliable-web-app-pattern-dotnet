{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "adoOrganizationUrl": {
        "value": "${AZURE_DEVOPS_ORG_URL}"
    },
    "adoToken": {
        "value": "${AZURE_DEVOPS_TOKEN}"
    },
    "clientIpAddress": {
        "value": "${AZD_IP_ADDRESS}"
    },
    "databasePassword": {
        "value": "$(secretOrRandomPassword ${AZURE_OPS_VAULT_NAME} Relecloud--SqlAdministratorPassword)"
    },
    "differentiator": {
        "value": "${AZURE_CI_DIFFERENTIATOR=none}"
    },
    "enableTelemetry": {
      "value": "${ENABLE_TELEMETRY=true}"
    },
    "environmentName": {
        "value": "${AZURE_ENV_NAME}"
    },
    "environmentType": {
        "value": "${AZURE_ENV_TYPE=dev}"
    },
    "githubRepositoryUrl": {
        "value": "${GITHUB_REPO}"
    },
    "githubToken": {
        "value": "${GITHUB_TOKEN}"
    },
    "jumphostAdministratorPassword": {
        "value": "$(secretOrRandomPassword ${AZURE_OPS_VAULT_NAME} Jumphost--AdministratorPassword)"
    },
    "location": {
        "value": "${AZURE_LOCATION}"
    },
    "principalId": {
        "value": "${AZURE_PRINCIPAL_ID}"
    },
    "principalType": {
        "value": "${AZURE_PRINCIPAL_TYPE=User}"
    },
    "deployHubNetwork": {
        "value": "${DEPLOY_HUB_NETWORK=false}"
    },
    "networkIsolation": {
        "value": "${NETWORK_ISOLATION=auto}"
    },
    "ownerEmail": {
        "value": "${OWNER_EMAIL=noreply@contoso.com}"
    },
    "ownerName": {
        "value": "${OWNER_NAME=notset}"
    },
    "secondaryAzureLocation": {
      "value": "${SECONDARY_AZURE_LOCATION}"
    },
    "useCommonAppServicePlan": {
        "value": "${COMMON_APP_SERVICE_PLAN=auto}"
    }
  }
}